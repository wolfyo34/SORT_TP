  "scripts": {
    "start": "node index.js",
    "test": "mocha --recursive",
    "code-style": "xo"
  },
  "dependencies": {
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.1",
    "@tensorflow/tfjs-node-gpu": "^3.3.0",
    "bluebird": "^3.7.2",
    "chai": "^4.3.4",
